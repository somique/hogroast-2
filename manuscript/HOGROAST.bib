@article{scannellRevisitPEGInducedPrecipitation2021,
  title = {Revisit {{PEG-Induced Precipitation Assay}} for {{Protein Solubility Assessment}} of {{Monoclonal Antibody Formulations}}},
  author = {Scannell, Martha J and Hyatt, Matthew W and Budyak, Ivan L and Woldeyes, Mahlet A and Wang, Ying},
  year = {2021},
