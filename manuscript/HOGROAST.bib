@article{wangConformationalSelectionInduced2013,
  title = {Conformational Selection and Induced Fit in Specific Antibody and Antigen Recognition: {{SPE7}} as a Case Study},
  shorttitle = {Conformational Selection and Induced Fit in Specific Antibody and Antigen Recognition},
  author = {Wang, Wei and Ye, Wei and Yu, Qingfen and Jiang, Cheng and Zhang, Jian and Luo, Ray and Chen, Hai-Feng},
