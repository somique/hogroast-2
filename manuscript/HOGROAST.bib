  author = {Frentzas, Sophia and Ahern, Elizabeth Stephanie and Weickhardt, Andrew James and Haydon, Andrew Mark and {de Souza}, Paul L. and Powderly, John D. and Wyant, Timothy and Tang, Jenny and Richards, Lori and Knickerbocker, Aron and Amit, Inbar and Ofran, Yanay and Vasselli, James Robert},
  year = {2023},
  month = jun,
  journal = {Journal of Clinical Oncology},
  volume = {41},
