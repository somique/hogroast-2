@article{lindorff-larsenStructureDynamicsUnfolded2012,
  title = {Structure and Dynamics of an Unfolded Protein Examined by Molecular Dynamics Simulation},
  author = {{Lindorff-Larsen}, Kresten and Trbovic, Nikola and Maragakis, Paul and Piana, Stefano and Shaw, David E.},
  year = {2012},
